@using Compent.uCommunity.Core.Comments
@using Compent.uCommunity.Core.Subscribe
@using uCommunity.Core.Activity
@using uCommunity.Subscribe

@{
    var subscribeService = DependencyResolver.Current.GetService<ISubscribeService>();

    ISubscribable activity = new CustomSubscribable { Id = SubscribeTestConstants.ActivityId, Type = IntranetActivityTypeEnum.News, Subscribers = Enumerable.Empty<Subscribe>() };
    subscribeService.FillSubscribers(activity);
}

@{ Html.RenderAction("Index", "Subscribe", new { subscribe = activity, activityId = SubscribeTestConstants.ActivityId }); }
@{ Html.RenderAction("Overview", "Subscribe", new { activityId = SubscribeTestConstants.ActivityId }); }