@model IEnumerable<uIntra.Navigation.MyLinkItemViewModel>

<li class="hidden"></li>
@foreach (var item in Model)
{
    <li class="side-nav__item">
        <a class="side-nav__link" href="@item.Url">
            @item.Name
            <span class="icon-close js-myLinks-remove" data-is-current-page="@item.IsCurrentPage" data-url="@Url.Action("Remove", "MyLinks", new { id = item.Id})"></span>
        </a>
    </li>
}