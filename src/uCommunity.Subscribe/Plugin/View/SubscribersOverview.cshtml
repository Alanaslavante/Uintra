@using uCommunity.Core.Extentions
@model uCommunity.Subscribe.SubscribeOverviewModel
@{
    var title = Model.Title.IsNullOrEmpty() ? Html.Localize("SubscribersOverView.Title.lbl") : Model.Title;
}
<div class="content__item-info js-subscribe-overview" style="display: none" data-activity-id="@Model.ActivityId">
    <input type="hidden" class="js-subscribe-version" />
    <h3 class="content__item-subtitle js-subscribe-title" data-text="@title"></h3>
    <ul class="content__item-list js-subscribe-list"></ul>
</div>
