@using uIntra.Core.Extentions
@model uIntra.Core.BrowserCompatibility.Models.BrowserCompatibilityModel
@if (@Model.BrowserNotSupported && @Model.ShowNotification)
{
    <div>
        <input type="button" class="js-disable-browser-compatibility-notification" />
        @Html.Localize("BrowserCompatibility.Notification.Message")
    </div>
}