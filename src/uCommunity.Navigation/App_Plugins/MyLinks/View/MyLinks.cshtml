@using uCommunity.Navigation.DefaultImplementation
@model MyLinksViewModel

<ul class="side-nav">
    <span>My links</span>
    <span class="side-nav__arrow js-side-nav__opener"></span>

    @foreach (var item in Model.MyLinks)
    {
        <li class="side-nav__item js-side-nav__item">
            <a class="side-nav__link js-side-nav__link" href="@item.Url">
                @item.Name
            </a>
        </li>
    }

    <span>Add page to my links</span>
</ul>
