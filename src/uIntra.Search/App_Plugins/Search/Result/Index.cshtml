@using uIntra.Search.Core
@model uIntra.Search.Core.SearchViewModel

<div id="js-search-page" class="search-result">
    
    @Html.Hidden("query", Model.Query)

    <div class="search-result__textbox-holder">
        <input class="search-result__textbox" type="search" id="searchbox" value="@Model.Query" />
        <span class="icon-search"></span>
    </div>


    <div id="search-result">
        @{ Html.RenderPartial("~/App_Plugins/Search/Result/SearchResult.cshtml", new List<SearchTextResultModel>());}
    </div>

</div>
