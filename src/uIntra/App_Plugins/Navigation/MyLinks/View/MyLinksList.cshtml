@model IEnumerable<uIntra.Navigation.MyLinkItemViewModel>

<li class="hidden"></li>
@foreach (var item in Model)
{
    <li data-id="@item.ContentId" class="side-nav__item">
        <a class="side-nav__link" href="@item.Url">
            @item.Name
            <span class="icon-close js-myLinks-remove" data-id="@item.Id" data-url="@Url.Action("Remove", "MyLinks", new { id = item.Id})"></span>
        </a>
    </li>
}